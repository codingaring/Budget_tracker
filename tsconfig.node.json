{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#assets/*": ["src/assets/*"],
      "#components/*": ["src/components/*"],
      "#constants/*": ["src/constants/*"],
      "#pages/*": ["src/pages/*"],
      "#styles/*": ["src/styles/*"],
      "#hooks/*": ["src/hooks/*"],
      "#utils/*": ["src/utils/*"],
      "#/": ["src/*"],
      "#electron/*": ["electron/*"]
    }
  },
  "include": ["vite.config.ts", "src"]
}
